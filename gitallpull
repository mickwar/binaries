#!/bin/bash

# for pulling from all of my repositores at github (would need to modify as new repositories are added)

DIR=`pwd`
LOCAL_REPO=(
    ~/bin
    ~/files/python
    ~/files/R
    ~/files/ubuntu-config
    ~/files/data
    ~/files/julia
    ~/files/C
    ~/files/homework
    ~/files/latex
    )

for repository in ${LOCAL_REPO[@]}
do
    cd $repository
    echo Pulling from $repository
    git pull
    echo ""
done

cd $DIR

